R:(C-O)/O*100,COLORSTICK;
RR:(C-REF(C,1))/REF(C,1)*100,POINTDOT,COLORBLACK;
CNT2:= IF(PERIOD=5, 1250, 520);
H_:HOD(R,CNT2),COLORGREEN,NODRAW;
L_:LOD(R,CNT2),COLORRED,NODRAW;
STICKLINE(H_ < 4 OR L_ < 4, 0, R, 0.1, 0),COLORBLACK;

LC:=REF(CLOSE,1);
TEMP1:=MAX(CLOSE-LC,0);
TEMP2:=ABS(CLOSE-LC);
RS:SMA(TEMP1,10,1)/SMA(TEMP2,10,1)*100-50,COLORLIGRAY;
_:IF(RS>20 OR RS<-20,RS,DRAWNULL),COLORGRAY;